<h1>食べたごはんの記録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@dish_image, url: record_path) do |f| %>
      <%= render 'shared/error_messages' %>
    
      <%= f.label :dish_id, "料理ID" %>
      <%= f.text_field :dish_id, class: 'form-control'  %>
      
      <%= f.file_field :image, class: 'form-control'  %>

      <%= f.submit "登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>